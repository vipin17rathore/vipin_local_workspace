package programs;

public class Prime_No {

	public static void main(String[] args) {
	
		boolean flag = true;
		for(int i=1;i<15;i++){
			for(int j=2;j<i;j++) {
				if(j%i==0) {
					flag=false;
					System.out.println("not prime"+i);
					break;
				}
			}
			if(flag==true) 
			{
				System.out.println("even"+i);
			}

		}
			}
	
}