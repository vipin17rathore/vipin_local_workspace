package programs;

public class Prime_No {

	public static void main(String[] args) {
	
	
		for(int i=1;i<15;i++){
			boolean flag=true;
			for(int j=2;j<i;j++) {
				if(i%j==0) {
					flag = false;
					System.out.println("not prime"+i);
					break;
				}

			}
			if(flag) 
			{
				System.out.println("even"+i);
			}
			
		}
			}
	
}