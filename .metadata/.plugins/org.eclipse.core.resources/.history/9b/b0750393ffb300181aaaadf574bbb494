package collection;
import java.util.ArrayList;


public class ArrayList1 {

	public static void main(String[] args) {
		ArrayList<String> ar = new ArrayList<String>();
		ar.add("viin");
		ar.add("rahul");
		ar.add("abhi");
		
		for (int i=0;i<ar.size();i++){
			System.out.println("ar"+ar);
		}
	}
}
