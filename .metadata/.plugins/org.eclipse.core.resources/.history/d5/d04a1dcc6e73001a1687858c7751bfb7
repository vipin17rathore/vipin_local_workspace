package java8_new;

public class TestClass implements Default_Method{

	@Override
	public void show() {
		System.out.println("show method");
	}
	
	@Override
	public void run() {
			System.out.println("inside test run");
	}

	public static void main(String a[]) {
		TestClass c = new TestClass();
		c.show();
		c.run();
	}
}
